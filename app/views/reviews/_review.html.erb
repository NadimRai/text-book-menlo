<p><%= review.rating %></p>
<p><%= review.comment %></p>

<% if user_signed_in? %>
	<% if review.user_id == current_user.id %>		
		<%= link_to "Edit", edit_listing_review_path(review.listing, review), class: "btn btn-info" %>
		<%= link_to "Delete", listing_review_path(review.listing, review), method: :delete, data: { confirm: "Are you sure?" }, class: "btn btn-danger" %>	
	<% end %>
<% end %>